{
    "name": "spatie/laravel-endpoint-resources",
    "description": "Add endpoints to your Laravel Resources without a hassle",
    "keywords": [
        "spatie",
        "laravel-endpoint-resources"
    ],
    "homepage": "https://github.com/spatie/laravel-endpoint-resources",
    "license": "MIT",
    "authors": [
        {
            "name": "Ruben Van Assche",
            "email": "ruben@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/database": "~5.8.0",
        "illuminate/routing": "~5.8.0"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "orchestra/testbench": "~3.8.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\LaravelEndpointResources\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\LaravelEndpointResources\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
}
